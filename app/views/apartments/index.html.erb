<h1>Apartments</h1>
    <table>
        <thead>
            <tr>
                <th>Number</th>
                <th>Building</th>
                <th colspan="3"></th>
            </tr>
        </thead>
        <tbody>
            <% @apartments.each do |apartment| %>
                <tr>
                    <td><%= apartment.number %></td>
                    <td><%= apartment.building.to_s %></td>
                    <td><%=link_to 'Show', apartment%></td>
                </tr>
            <% end %>
        </tbody>
    </table>
    <%= link_to 'New Apartment', new_apartment_path %>